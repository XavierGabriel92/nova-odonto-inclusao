generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  shadowDatabaseUrl    = env("SHADOW_DATABASE_URL")
  referentialIntegrity = "prisma"
}

model Company {
  id               Int                @id @default(autoincrement())
  name             String
  cpnj             String
  password         String
  user             User[]
  userDraft             UserDraft[]
}

model User {
  company          Company            @relation(fields: [companyId], references: [id])
  companyId        Int
  id               Int                @id @default(autoincrement())
  titular          String
  matricula        String
  nome             String
  nascimento       String   
  rg               String?
  rgExpedicao      String?  
  rgEmissor        String?
  rgUf             String?
  cpf              String
  sexo             String
  parentesco       String?
  dataAdimissao    String?   
  estadoCivil      String
  cep              String
  logradouro       String
  numero           String
  complemento      String?
  bairro           String
  cidade           String
  estado           String
  fone             String?
  plano            String
  nomeMae          String
  doencasPre       String?
  dataVigencia     String?   
  dataCancelamento          String?   
  email                     String?
  celular                   String?
  nomePai                   String?
  cns                       String?
  nomeTitular               String
  movimento                 String
  origemCarencia            String?
  cid                       String?
  status                    String
  codMotivo                 String?
  codBeneficiarioDependente String?
  dataObito                 String?   
  nroObito                  String?
  nroNascido                String?
  dataAposentadoria         String?   
  createdAt        String           
  updatedAt        String           
}

model UserDraft {
  company          Company            @relation(fields: [companyId], references: [id])
  companyId        Int
  id               Int                @id @default(autoincrement())
  titular          String
  matricula        String
  nome             String
  nascimento       String
  rg               String?
  rgExpedicao      String?
  rgEmissor        String?
  rgUf             String?
  cpf              String
  sexo             String
  parentesco       String?
  dataAdimissao    String?
  estadoCivil      String
  cep              String
  logradouro       String
  numero           String
  complemento      String?
  bairro           String
  cidade           String
  estado           String
  fone             String?
  plano            String
  nomeMae          String
  doencasPre       String?
  dataVigencia     String? 
  dataCancelamento          String? 
  email                     String?
  celular                   String?
  nomePai                   String?
  cns                       String?
  nomeTitular               String
  origemCarencia            String?
  cid                       String?
  status                    String
  codMotivo                 String?
  codBeneficiarioDependente String?
  dataObito                 String?
  nroObito                  String?
  nroNascido                String?
  dataAposentadoria         String? 
  createdAt        String
  updatedAt        String
}